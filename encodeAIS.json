        "longitude": {
          "description": "Longitude of the ship",
          "type": "number",
          "minimum": -180,
          "maximum": 180
        },
        "latitude": {
          "description": "Latitude of the ship",
          "type": "number",
          "minimum": -90,
          "maximum": 90
        },
        "course": {
          "description": "Course of the ship",
          "type": "number",
          "minimum": 0,
          "maximum": 3600
        },
        "trueHeading": {
          "description": "True heading of the ship",
          "type": "number",
          "minimum": 0,
          "maximum": 359
        },
        "timestamp": {
          "description": "Timestamp of the AIS message",
          "type": "number",
          "minimum": 0,
          "maximum": 63
        }
    },
    "required": [
      "status",
      "speed",
      "longitude",
      "latitude",
      "course",
      "trueHeading",
      "timestamp"
    ]
  },
  "user": {
    "type": "object",
    "properties": {
      "apiKey": {
        "description": "API key of the user",
        "type": "string",
        "maxLength": 50
      }
    },
    "required": [
      "apiKey"
    ]
  }
},
"required": [
"ship",
"AIS",
"user"
]
}
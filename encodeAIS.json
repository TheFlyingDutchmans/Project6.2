        }
      },
      "required": [
        "mmsi"
      ]
    },
    "AIS": {
      "type": "object",
      "properties": {
        "status": {
          "type": "number",
          "description": "Status of the ship",
          "minimum": 0,
          "maximum": 15
        },
        "speed": {
          "description": "Speed of the ship",
          "type": "number",
          "minimum": 0,
          "maximum": 1022
